        <jsScript_script>&#47;&#47;Script here

&#47;&#47; cantPersonas
var cantPersonas = 1;

var arrAniosAprobados  = [1, E51_1, E51_2, E51_3, E51_4, E51_5, E51_6, E51_7, E51_8, E51_9, E51_10, E51_11];

&#47;&#47; fk_educacion
var nivelEducacionAlcanzado;
for (var i = 0; i&lt;arrAniosAprobados.length; i++){
  if (parseInt(arrAniosAprobados[i], 10) != 0){
    nivelEducacionAlcanzado = i;
  }
}
nivelEducacionAlcanzado++; &#47;&#47; En la tabla no hay id 0

var nivelEducacionFinalizado = 0;
if (parseInt(E53, 10) == 1) {
  nivelEducacionFinalizado = nivelEducacionAlcanzado;
} else {
  var counter = 1;
  for (var i=11; i&gt;=0; i--) {
   if (counter &gt;= 0 &amp;&amp; parseInt(arrAniosAprobados[i], 10) != 0) {
     counter--;
     nivelEducacionFinalizado = i;
   }
  }
  nivelEducacionFinalizado++;
}
var fk_educacion = nivelEducacionFinalizado;

&#47;&#47; fk_sexos
var fk_sexos = parseInt(E26, 10);

&#47;&#47; fk_edades
var fk_edades;
if (parseInt(E27, 10) &lt;= 14) {
 fk_edades = 1;
} else if (parseInt(E27, 10) &lt;= 18) {
 fk_edades = 2;
} else if (parseInt(E27, 10) &lt;= 22) {
 fk_edades = 3;
} else if (parseInt(E27, 10) &lt;= 30) {
 fk_edades = 4;
} else if (parseInt(E27, 10) &lt;= 40) {
 fk_edades = 5;
} else if (parseInt(E27, 10) &lt;= 50) {
 fk_edades = 6;
} else if (parseInt(E27, 10) &lt;= 60) {
 fk_edades = 7;
} else if (parseInt(E27, 10) &lt;= 70) {
 fk_edades = 8;
} else {
 fk_edades = 9;
}

&#47;&#47; fk_tiempo, fk_geografia, idPersona
var fk_tiempo = parseInt(MES, 10) + 12;
var fk_geografia = parseInt(&quot;2010&quot; + parseInt(NUMERO, 10));
var idPersona = parseInt(NPER, 10);

&#47;&#47; fk_tics
var fk_tics = 88888;
if (parseInt(E62, 10) != 1){
  fk_tics = 5;
} else{
  fk_tics = parseInt(E65, 10);
}
if (parseInt(E60, 10) == 1) { 
  fk_tics = fk_tics + 5;
}</jsScript_script>
