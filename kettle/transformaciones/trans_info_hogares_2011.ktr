        <jsScript_script>&#47;&#47;Script here

&#47;&#47;Calculo nivelConfort
function tieneElectro(codigo){
  return (codigo == 1);
}

var cantElectro = 0;
var arrElectro = [D21_1, D21_2, D21_3, D21_4, D21_5, D21_6, D21_7, D21_8, D21_9, D21_10,
 D21_11, D21_12, D21_13, D21_14, D21_15, D21_16, 
 D21_17, D21_18, D21_19]

for (var i=0; i&lt;arrElectro.length; i++) {
  if (tieneElectro(arrElectro[i])) {
    cantElectro = cantElectro + 1;
  }
}

var tipoAgua = D11;

var aguaMala = (tipoAgua == 4 || tipoAgua == 5 || tipoAgua == 6)
var fk_niveles_confort = null;
if (cantElectro &lt;= 5){
  fk_niveles_confort = 1;
} 
if (cantElectro &gt; 5 &amp;&amp; cantElectro &lt;= 10){
  if (aguaMala){
    fk_niveles_confort = 1;
  } else {
    fk_niveles_confort = 2;
  }
}
if (cantElectro &gt; 10 &amp;&amp; cantElectro &lt;= 15){
  if (aguaMala){
    fk_niveles_confort = 1;
  } else {
    fk_niveles_confort = 3;
  }
}
if (cantElectro &gt; 15){
  if (aguaMala){
    fk_niveles_confort = 1;
  } else {
    fk_niveles_confort = 4;
  }
}

&#47;&#47;idTiempo
var fk_tiempo = parseInt(MES) + 24;

&#47;&#47;ifGeografia
var fk_geografia = parseInt(NUMERO); &#47;&#47;Ya viene con el 2011

&#47;&#47;idTipoVivienda
var fk_tipos_vivienda = null;
if (C1 == 1){
  fk_tipos_vivienda = 1;
} else if (C1 == 5){
    fk_tipos_vivienda = 3;
} else {
    fk_tipos_vivienda = 2;
}

var cantHogares = 1; &#47;&#47;??
var cantHombres = HT1;
var cantMujeres = HT2;
var cantPersonas = cantHombres + cantMujeres;
var cantPersonasMenor14 = HT3;
var cantPersonasMayor14 = cantPersonas - cantPersonasMenor14;
var cantOcupados = HT5;
var cantDesocupados = HT6;</jsScript_script>
