        <jsScript_script>&#47;&#47;Script here

function buildId2011(numero){
  var anio = numero.slice(0,4);
  if (anio == &quot;2011&quot;){
    var tail = parseInt(numero.substr(4, numero.length), 10) &#47;&#47; numero sin el 2011 del principio ni los ceros concatenados
    return parseInt(&quot;2011&quot; + tail, 10);
  } else{
    return parseInt(numero, 10);
  }
}

&#47;&#47; cantPersonas
var cantPersonas = 1;

&#47;&#47; fk_educacion
var fk_educacion_finalizada;
if (parseInt(E49, 10) == 2){
  fk_educacion_finalizada = 1;
} else {
  var nivelEducacionFinalizado = -1;
  var arrAniosFinalizados = [E197_1, E201_1, E212_1, E215_1, E218_1, E221_1, E224_1];
  for (var i=arrAniosFinalizados.length - 1; i&gt;=0; i--) {
   if (parseInt(arrAniosFinalizados[i], 10) == 1) {
     nivelEducacionFinalizado = i;
     break;
   }
  }
  if (nivelEducacionFinalizado == -1){
    fk_educacion_finalizada = 1;
  } else if (nivelEducacionFinalizado == 0){
    if (parseInt(E51_2, 10) &gt;= 0){
      fk_educacion_finalizada = 3; 
    } else {
      fk_educacion_finalizada = 4;
    }
  } else if (nivelEducacionFinalizado == 1){
    if (parseInt(E51_5, 10) &gt;= 0){
      fk_educacion_finalizada = 6;
    } else if (parseInt(E51_6, 10) &gt;= 0){
      fk_educacion_finalizada = 7;
    } else {
      fk_educacion_finalizada = 5;
    }
  } else if (nivelEducacionFinalizado &gt;= 2){
    fk_educacion_finalizada = nivelEducacionFinalizado + 6;
  }
}

var fk_educacion = fk_educacion_finalizada;
&#47;&#47; fk_sexos
var fk_sexos = parseInt(E26, 10);

&#47;&#47; fk_edades
var fk_edades;
if (parseInt(E27, 10) &lt;= 14) {
 fk_edades = 1;
} else if (parseInt(E27, 10) &lt;= 18) {
 fk_edades = 2;
} else if (parseInt(E27, 10) &lt;= 22) {
 fk_edades = 3;
} else if (parseInt(E27, 10) &lt;= 30) {
 fk_edades = 4;
} else if (parseInt(E27, 10) &lt;= 40) {
 fk_edades = 5;
} else if (parseInt(E27, 10) &lt;= 50) {
 fk_edades = 6;
} else if (parseInt(E27, 10) &lt;= 60) {
 fk_edades = 7;
} else if (parseInt(E27, 10) &lt;= 70) {
 fk_edades = 8;
} else {
 fk_edades = 9;
}

&#47;&#47; fk_tiempo, fk_geografia
var fk_tiempo = parseInt(MES, 10) + 24;
var fk_geografia = buildId2011(NUMERO);

&#47;&#47; fk_tics
var fk_tics = 88888;
if (parseInt(E62, 10) != 1){
  fk_tics = 5;
} else{
  fk_tics = parseInt(E65, 10);
}
if (parseInt(E60, 10) == 1) { 
  fk_tics = fk_tics + 5;
}</jsScript_script>
